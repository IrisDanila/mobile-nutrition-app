{
  "expo": {
    "name": "FoodScanAI",
    "slug": "food-scan-ai",
    "scheme": "foodscanai",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#111827"
      },
      "permissions": ["CAMERA"]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
  }
  }
}
